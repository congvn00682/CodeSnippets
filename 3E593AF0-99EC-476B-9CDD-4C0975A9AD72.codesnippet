<plist version="1.0">
<dict>
	<key>IDECodeSnippetCompletionPrefix</key>
	<string>call ClassNameGenerator</string>
	<key>IDECodeSnippetCompletionScopes</key>
	<array>
		<string>All</string>
	</array>
	<key>IDECodeSnippetContents</key>
	<string>import Foundation

struct ClassNameGenerator {

    static func generate&lt;T: AnyObject&gt;(class: T.Type) -&gt; String {
        let joinedBundleIDAndClassName = NSStringFromClass(T.self)
        let bundleIDAndClassName = joinedBundleIDAndClassName.split(separator: ".")
        let className = String(bundleIDAndClassName.last ?? "")
        return className
    }

}</string>
	<key>IDECodeSnippetIdentifier</key>
	<string>3E593AF0-99EC-476B-9CDD-4C0975A9AD72</string>
	<key>IDECodeSnippetLanguage</key>
